{
  "name": "telegram-sticker-bot",
  "version": "1.0.0",
  "description": "Telegram bot for creating sticker packs from photos using AI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "pnpm exec prisma generate",
    "db:migrate": "pnpm exec prisma migrate dev",
    "db:push": "pnpm exec prisma db push",
    "db:studio": "pnpm exec prisma studio"
  },
  "keywords": [
    "telegram",
    "bot",
    "sticker",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@grammyjs/hydrate": "^1.4.1",
    "@prisma/client": "^5.8.1",
    "dotenv": "^16.4.1",
    "grammy": "^1.21.1",
    "replicate": "^1.4.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "prisma": "^5.8.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}